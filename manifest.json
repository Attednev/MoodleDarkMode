{
    "manifest_version": 2,
    "name": "MoodleDarkMode",
    "version": "0.0.2",
    "description": "Adds a dark mode for all webpages matching moodle.tum.de",
    "icons": {
        "235": "icons/icon-235.png"
    },
    "content_scripts": [
        {
            "matches": [ "*://*.moodle.tum.de/*" ],
            "js": [ "darkMode.js" ],
            "css": [ "darkMode.css" ]
        }
    ]
}